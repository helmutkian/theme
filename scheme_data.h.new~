#ifndef SCHEME_DATA_H
#define SCHEME_DATA_H

typedef enum {
  SCHEME_FLOAT
  , SCHEME_FIXNUM
  , SCHEME_CHAR
  , SCHEME_STRING
  , SCHEME_CONS
  , SCHEME_NIL
} scheme_type_t;

typedef struct scheme_data_s {
  scheme_type_t type;
  union {
    double f;
    int i;
    char c;
    struct {
      scheme_data_s *car;
      scheme_data_s *cdr;
    } cons;
    struct {
      char *arr;
      int length;
    } s;
      

#endif
